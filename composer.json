{
  "require": {
    "craftcms/cms": "3.4.30",
    "craftcms/redactor": "^2.6",
    "misterbk/mix": "^1.5",
    "mmikkel/reasons": "2.1.2",
    "putyourlightson/craft-elements-panel": "1.1.0",
    "verbb/super-table": "2.5.2",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
